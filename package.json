  "build": {
    "generateUpdatesFilesForAllChannels": true,
    "artifactName": "Dipperin-Wallet-${os}-${version}.${ext}",
    "appId": "com.dipperin.wallet",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "appImage": {
      "systemIntegration": "ask",
      "desktop": {
        "Terminal": "false",
        "Categories": "Network"
      }
    },
    "win": {
      "target": [
        {
          "target": "zip"
        },
        {
          "target": "nsis"
        }
      ]
    },
    "nsisWeb": {},
    "linux": {
      "desktop": {
        "StartupWMClass": "Dipperin-Wallet"
      },
      "category": "Network",
      "target": [
        {
          "target": "deb"
        },
        {
          "target": "AppImage"
        }
      ],
      "icon": "public/icons"
    },
    "mac": {
      "category": "public.app-category.finance",
      "target": [
        {
          "target": "zip"
        }
      ]
    },
    "publish": [
      {
        "provider": "github",
        "token": "c88408b40f6572833f6fd3c5a868ada9c9cee7f6",
        "private": true,
        "vPrefixedTagName": true,
        "publishAutoUpdate": false
      }
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dipperin": {
    "version": "1.1.0",
    "server": "114.119.116.157:9000"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "flow": false,
          "typescript": true
        }
      ]
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
